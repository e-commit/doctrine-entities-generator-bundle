{% block field_hint %}
    {% set block_field_hint_name = 'field_hint_'~fieldMapping.type %}
    {% if sourceType|length > 0 %}
        {{ sourceType }}
    {% elseif block(block_field_hint_name) is defined %}
        {{ block(block_field_hint_name)|spaceless }}
    {% elseif type %}
        {% set hintParts = [] %}
        {% if helper.is(type, 'Symfony\\Component\\TypeInfo\\Type\\UnionType') %}
            {% set types = type.types  %}
        {% else %}
            {% set types = [type] %}
        {% endif %}

        {% for subType in types %}
            {% if 'null' != subType.__toString() %}
                {% if helper.is(subType, 'Symfony\\Component\\TypeInfo\\Type\\ObjectType') %}
                    {% set subTypeHint = '\\' ~ subType.className %}
                {% elseif helper.is(subType, 'Symfony\\Component\\TypeInfo\\Type\\CollectionType') and subType.isIdentifiedBy(constant('Symfony\\Component\\TypeInfo\\TypeIdentifier::ARRAY')) %}
                    {% set subTypeHint = 'array' %}
                {% else %}
                    {% set subTypeHint = subType.__toString() %}
                {% endif %}

                {% set hintParts = hintParts|merge([subTypeHint]) %}
            {% endif %}
        {% endfor %}

        {% if hintParts|length > 1 %}
            {{ hintParts|join('|') }}|null
        {% else %}
            {{ helper.isNullable(reflectionProperty) ? '?' : '' }}{{ hintParts|join('|') }}
        {% endif %}
    {% endif %}
{% endblock %}

{% block field_set %}
{% set phpdoc = phpDocPropertyType|spaceless %}
{% if phpdoc|length > 0 %}
    /**
     * @param {{ phpdoc }} ${{ variableName }}
     */
{% endif %}
    public function {{ methodName }}({{ (block('field_hint')|spaceless) ? block('field_hint')|spaceless ~ ' ' : '' }}${{ variableName }}): self
    {
        $this->{{ fieldName }} = ${{ variableName }};

        return $this;
    }
{% endblock %}

